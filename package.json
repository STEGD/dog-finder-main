{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React and Express apps",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node functions/index.js",
    "server": "nodemon functions/index.js",
    "client": "npm start --prefix client",
