  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "crawler": "^1.2.2",
    "express": "^4.16.2",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "link-check": "^4.5.1",
    "table": "^6.0.3",
    "website-scraper": "^4.2.3"
  }
}
